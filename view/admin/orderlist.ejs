        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Order List </h2>
                    <p>Lorem ipsum dolor sit amet.</p>
                </div>
                <div>
                    <input type="text" placeholder="Search order ID" class="form-control bg-white">
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-6 col-md-3">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>#ID</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Total</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Payment Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (let i = 0; i < orders.length; i++) { %>
                                <tr>
                                    <td><%= orders[i].orderId %></td>
                                    <td><b><%= orders[i].user.name %></b></td>
                                    <td><%= orders[i].user.email %></td>
                                    <td>$<%= orders[i].totalAmount %></td>
                                    <td><span class="badge rounded-pill alert-warning"><%= orders[i].status %></span></td>
                                    <td><span class="badge badge-pill badge-soft-success"><%= orders[i].paymentStatus %></span></td>
                                    <td><%= orders[i].orderDate.toDateString() %></td>
                                    <td class="text-end">
                                        <a href="/admin/orderdetail?orderId=<%= orders[i]._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                        <div class="dropdown">
                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
                                            <div class="dropdown-menu">
                                                <a class="dropdown-item" href="/admin/orderstatus?orderId=<%= orders[i]._id%>&status=Pending " onclick="confirmStatusChange('Pending')">Pending</a>
                                                <a class="dropdown-item text" href="/admin/orderstatus?orderId=<%= orders[i]._id%>&status=processing" onclick="confirmStatusChange('Processing')">Processing</a>
                                                <a class="dropdown-item text" href="/admin/orderstatus?orderId=<%= orders[i]._id%>&status=compleated"onclick="confirmStatusChange('compleated')">compleated</a>
                                                <a class="dropdown-item text" href="/admin/orderstatus?orderId=<%= orders[i]._id%>&status=failed" onclick="confirmStatusChange('failed')">failed</a>
                                                <a class="dropdown-item text-danger" href="/admin/orderstatus?orderId=<%= orders[i]._id%>&status=canceled" onclick="confirmStatusChange('canceled')">canceled</a>
                                            </div>

                                        </div> <!-- dropdown //end -->

                                            <% if (orders[i].is_block === 0) { %>
                                                <a href="#" class="btn btn-sm font-sm rounded btn-danger" onclick="toggleBlock('<%= orders[i]._id %>', '<%= orders[i].is_block %>')">
                                                    <i class="material-icons md-edit"></i> Block
                                                </a>
                                            <% } else { %>
                                                <a href="#" class="btn btn-sm font-sm rounded btn-success" onclick="toggleBlock('<%= orders[i]._id %>', '<%= orders[i].is_block %>')">
                                                    <i class="material-icons md-edit"></i> Unblock
                                                </a>
                                            <% } %>
                                        </td>
                                    </tr>   
                                <% } %>                      
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                    <div class="pagination-area mt-30 mb-50">
                        <nav aria-label="Page navigation example">
                            <ul class="pagination justify-content-start">
                                <% for (let i = 1; i <= totalPages; i++) { %>
                                    <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                        <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                                    </li>
                                <% } %>
                                <!-- Add a "..." link for skipping pages -->
                                <% if (currentPage + 1 < totalPages) { %>
                                    <li class="page-item">
                                        <a class="page-link dot" href="#">...</a>
                                    </li>
                                <% } %>
                                <!-- Next page link -->
                                <% if (currentPage < totalPages) { %>
                                    <li class="page-item">
                                        <a class="page-link" href="?page=<%= currentPage + 1 %>">
                                            <i class="material-icons md-chevron_right"></i>
                                        </a>
                                    </li>
                                <% } %>
                            </ul>
                        </nav>
                    </div>

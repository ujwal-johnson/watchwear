    <script>
        function confirmStatusChange(status) {
            return confirm(`Are you sure you want to change the status to ${status}?`);
        }
    </script>
    <script>
        function toggleBlock(orderId, isBlock) {
            if (confirm(`Are you sure you want to ${isBlock ? 'unblock' : 'block'} this order?`)) {
                window.location.href = `/admin/blockorder?orderId=${orderId}`;
            }
        }
    </script>
    

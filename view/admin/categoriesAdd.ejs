                <script>
                    function validateForm() {
                        var name = document.getElementById('product_name').value;
                        var image = document.getElementById('product_image').value;
                        var gender = document.querySelector('input[name="gender"]:checked');
                        var description = document.querySelector('textarea[name="description"]').value;
            
                        // Validation for the Name field
                        if (name.trim() === '') {
                            document.getElementById('nameError').innerHTML = 'Name is required';
                            return false;
                        } else {
                            document.getElementById('nameError').innerHTML = '';
                        }
            
                        // Validation for the Image field
                        if (image.trim() === '') {
                            document.getElementById('imageError').innerHTML = 'Image is required';
                            return false;
                        } else {
                            document.getElementById('imageError').innerHTML = '';
                        }
            
                        // Validation for the Gender field
                        if (!gender) {
                            document.getElementById('genderError').innerHTML = 'Please select a gender';
                            return false;
                        } else {
                            document.getElementById('genderError').innerHTML = '';
                        }
            
                        // Validation for the Description field
                        if (description.trim() === '') {
                            document.getElementById('descriptionError').innerHTML = 'Description is required';
                            return false;
                        } else {
                            document.getElementById('descriptionError').innerHTML = '';
                        }
            
                        // Additional validation can be added based on your specific requirements
            
                        // If all validation passes, the form will be submitted
                        return true;
                    }
                </script>

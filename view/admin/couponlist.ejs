                </div>
                <div>
                    <a href="/admin/getAddCoupon" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            <div class="card mb-4">
                <header class="card-header">
                    <div class="row align-items-center">
                        <div class="col col-check flex-grow-0">
                            <div class="form-check ms-2">
                                <input class="form-check-input" type="checkbox" value="">
                            </div>
                        </div>
                        <div class="col-md-3 col-12 me-auto mb-md-0 mb-3">
                            <select class="form-select">
                                <option selected>All category</option>
                                <option>Electronics</option>
                                <option>Clothes</option>
                                <option>Automobile</option>
                            </select>
                        </div>
                        <div class="col-md-2 col-6">
                            <input type="date" value="02.05.2021" class="form-control">
                        </div>
                        <div class="col-md-2 col-6">
                            <select class="form-select">
                                <option selected>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                    </div>
                </header>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th>#ID</th>
                                <th class="align-middle" scope="col">Coupon Code</th>
                                <th class="align-middle" scope="col">Limit</th>
                                <th class="align-middle" scope="col">Expiry Date </th>
                                <th class="align-middle" scope="col">Discount</th>
                                <th style="width: 150px;" class="align-middle" scope="col">Status</th>
                                <th class="align-middle" scope="col">Type</th>
                                <th class="align-middle" scope="col">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% coupons.forEach((coupon, index) => { %>

                                <tr>
                                <td><%= index + 1 %></td>
                                <td><a href="#" class="fw-bold"><%= coupon.code %></a></td>
                                <td><%= coupon.limit %></td>
                                <td><%= coupon.expiry.toDateString() %></td>
                                <td>
                                <% if (coupon.type === 'fixed') { %>
                                ₹<%= coupon.discount %>
                                <% } else { %>
                                <%= coupon.discount %>%
                                                <% } %>
                                                </td>
                                                <td>
                                                <% if (coupon.isListed) { %>
                                                    <span class="badge badge-pill badge-soft-primary">Listed</span>
                                                <% } else { %>
                                                    <span class="badge badge-pill badge-soft-danger">Unlisted</span>
                                                <% } %>
                                                </td>
                                                <td><%= coupon.type %></td>
                                                <td>
                                                <a href="/admin/couponDetail?couponId=<%= coupon._id %>" class="btn btn-xs rounded font-xs">Details</a>
                                                <div class="dropdown">
                                                    <a href="/admin/couponstatus?couponId=<%= coupon._id %>" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-xs">
                                                        <i class="material-icons md-more_horiz"></i>
                                                    </a>
                                                    <div class="dropdown-menu">
                                                        <a class="dropdown-item font-xs" href="/admin/couponedit?couponId=<%= coupon._id %>">Edit</a>
                                                        <% if (coupon.isListed) { %>
                                                            <a href="/admin/couponstatus?couponId=<%= coupon._id %>&action=unlist">
                                                                <p class="text-danger ms-4 font-xs">Unlist</p>
                                                            </a>
                                                        <% } else { %>
                                                            <a href="/admin/couponstatus?couponId=<%= coupon._id %>&action=list">
                                                                <p class="text-primary ms-4 font-xs">List</p>
                                                            </a>
                                                        <% } %>
                                                    </div>
                                                </div>
                                                 <!-- dropdown //end -->
                                                </td>
                                            </tr>                                      
                                        <% }); %>                    
                        </tbody>
                    </table>
                </div> <!-- table-responsive //end -->
            </div> <!-- card-body end// -->   

            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-start">
                        <% for (let i = 1; i <= totalPages; i++) { %>
                            <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                            </li>
                        <% } %>
                        <!-- Add a "..." link for skipping pages -->
                        <% if (currentPage + 1 < totalPages) { %>
                            <li class="page-item">
                                <a class="page-link dot" href="#">...</a>
                            </li>
                        <% } %>
                        <!-- Next page link -->
                        <% if (currentPage < totalPages) { %>
                            <li class="page-item">
                                <a class="page-link" href="?page=<%= currentPage + 1 %>">
                                    <i class="material-icons md-chevron_right"></i>
                                </a>
                            </li>
                        <% } %>
                    </ul>
                </nav>

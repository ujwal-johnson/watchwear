<!-- Modal for Block/Unblock Confirmation -->
<div class="modal fade" id="blockModal" tabindex="-1" role="dialog" aria-labelledby="blockModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="blockModalLabel">Block/Unblock Confirmation</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <p id="blockModalMessage"></p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
          <a id="blockModalConfirmButton" href="#" class="btn btn-danger">Confirm</a>
        </div>
      </div>
    </div>
  </div>

  <!-- Include jQuery and Bootstrap JS -->
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>

<script>
  function showBlockModal(productId, action) {
    var blockModal = $('#blockModal');
    var confirmButton = $('#blockModalConfirmButton');
    var message = $('#blockModalMessage');

    message.text(`Are you sure you want to ${action.toLowerCase()} this product?`);
    
    // Update the href of the Confirm button based on the action
    confirmButton.attr('href', `/admin/blockproducts?productId=${productId}`);

    blockModal.modal('show');
  }
</script>


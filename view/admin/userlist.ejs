        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">User List</h2>
                    
                </div>
                <div>
                    <a href="#" class="btn btn-primary btn-sm rounded">Create new</a>
                </div>
            </div>
            
            <table class="table" style="width: 100%;">
                  <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    
                    <th scope="col">Mobile</th>
                    <th scope="col">Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% if (users.length > 0) {
                    for (let i = 0; i < users.length; i++) { %>
                      <tr>
                        <td><%= users[i].name %></td>
                        <td><%= users[i].email %></td>
                        <td><%= users[i].mobile %></td>
                        
                        
                        <!-- to block and unblock user -->

                           <td class="col-lg-2 col-sm-3 col-4 col-action text-end">
                              <% if (users[i].is_block == 0) { %>
                                <a href="/admin/blockuser?userId=<%= users[i]._id %>" class="btn btn-sm font-sm rounded btn-danger" style="background-color: green;border-color: green;">        
                                     <i class="material-icons md-edit"></i> Block
                                </a>
                              <% } else { %>
                                <a href="/admin/blockuser?userId=<%= users[i]._id %>" class="btn btn-sm font-sm rounded btn-danger">        
                                    <i class="material-icons md-edit"></i> Unblock
                               </a>
                              <% } %>
                          </td>
                          
                      </tr>
                    <% }
                  } else { %>
                    <tr>
                      <td colspan="6">Users Not Found</td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
              
            <div class="pagination-area mt-30 mb-50">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-start">
                        <% for (let i = 1; i <= totalPages; i++) { %>
                            <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                            </li>
                        <% } %>
                        <% if (currentPage + 1 < totalPages) { %>
                            <li class="page-item">
                                <a class="page-link dot" href="#">...</a>
                            </li>
                        <% } %>
                        <% if (currentPage < totalPages) { %>
                            <li class="page-item">
                                <a class="page-link" href="?page=<%= currentPage + 1 %>">
                                    <i class="material-icons md-chevron_right"></i>
                                </a>
                            </li>
                        <% } %>
                    </ul>
                </nav>
